{
  "imports": {
    "hono": "jsr:@hono/hono@^4.5.2",
    "@std/dotenv/load": "jsr:@std/dotenv/load"
  },
  "tasks": {
    "start": "deno run --allow-net --allow-read --allow-env app.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },"deploy": {
    "exclude": [".env"],
    "include": ["./jsons", "*.ts", "deno.json"],
    "entrypoint": "./app.ts"
  }
}
